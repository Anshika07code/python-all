questions = {
    "What is 2+2?": "4",
    "Capital of France?": "Paris",
    "Python is a ___?": "Programming language"
}
score = 0
for q, ans in questions.items():
    answer = input(q + " ")
    if answer.strip().lower() == ans.lower():
        print("Correct!")
        score += 1
    else:
        print("Wrong!")
print("Your score:", score)
